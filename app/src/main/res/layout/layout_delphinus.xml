            app:layout_constraintTop_toBottomOf="@id/trackLayout" />

        <Button
            android:background="#687B4A22"
            android:hint="@string/marzipan_sine_mournful_transfusion"
            android:id="@+id/recuseView"
            android:layout_height="wrap_content"
