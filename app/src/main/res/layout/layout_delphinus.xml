        android:layout_height="wrap_content"
        android:layout_margin="@dimen/fortescue_eulerian_margin"
        android:layout_width="wrap_content"
        android:text="@string/typescript_arduous_lufthansa"
        android:textColor="@color/ruminant_trollop_apostate"
        app:layout_constraintBottom_toTopOf="@id/fellFeeneyView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ostensibleGlycerolView" />

    <CheckedTextView
        android:background="@color/sabbatical_ethos"
        android:hint="@string/furman_talkie_anneal_fijian"
        android:id="@+id/fellFeeneyView"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="@string/healey_pfennig_dull_bold"
        android:textColor="#193D07C6"
