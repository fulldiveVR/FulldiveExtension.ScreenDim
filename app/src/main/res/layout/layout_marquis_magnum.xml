    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/zoneLayout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        app:layout_constraintBottom_toTopOf="@id/jacobiteWoreView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/slickCushmanView">

        <CheckBox
            android:background="@color/skunk_ncaa"
            android:hint="@string/linebacker_hamal_jeep_loire"
            android:id="@+id/hereinbelowContributionView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/hangdog_tyler_embezzle"
            android:textColor="@color/doom_account"
            app:layout_constraintBottom_toTopOf="@id/exorciseFaheyView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:background="@color/dominican_transylvania"
            android:hint="@string/lieberman_canis_judicatory"
