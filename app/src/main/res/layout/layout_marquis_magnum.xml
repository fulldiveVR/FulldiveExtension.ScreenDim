    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/listAblazeLayout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/slickCushmanView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <AutoCompleteTextView
            android:background="@color/wesleyan_aires_protean"
            android:hint="@string/aster_battlefront_bryophyta"
            android:id="@+id/carolView"
            android:layout_height="wrap_content"
