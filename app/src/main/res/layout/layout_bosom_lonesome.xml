        app:layout_constraintTop_toBottomOf="@id/stewTaillightView" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/leninistLayout"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        app:layout_constraintBottom_toTopOf="@id/balkanAircraftLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fallenHymnView">

        <EditText
            android:background="@color/checkbook_durer_occupy"
            android:hint="@string/banquet_clumsy_mudding_haste"
            android:id="@+id/sickbedView"
            android:inputType="textCapWords"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/quiescent_blackfoot_lundquist"
            android:textSize="@dimen/populate_accountant_textsize"
            app:layout_constraintBottom_toTopOf="@id/abstentionUsherLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:gravity="center_horizontal|end"
            android:id="@+id/abstentionUsherLayout"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layout_constraintBottom_toTopOf="@id/soundLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/sickbedView">

            <AutoCompleteTextView
                android:hint="@string/populism_molecule_floridian"
                android:id="@+id/recuperateView"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
