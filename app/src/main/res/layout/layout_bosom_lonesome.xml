            android:text="@string/pioneer_geography_mesa_alpert"
            android:textColor="@color/crafty_blind_graze"
            app:layout_constraintBottom_toTopOf="@id/giganticView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/venalAcheView" />

        <Button
            android:background="#698078"
            android:hint="@string/knott_inertia_officiate_jill"
            android:id="@+id/giganticView"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/sima_neumann_padding"
            android:layout_width="0dp"
            android:text="@string/discreet_conifer_bowline"
            android:textColor="@color/defy_degumming"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/airmanNectarView" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
