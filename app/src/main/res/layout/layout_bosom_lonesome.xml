            android:layout_width="match_parent"
            android:text="@string/wotan_swirl_ansi_diebold"
            android:textColor="@color/shakespearean_tacitus_carabao"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/hydroniumRebelledView" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/balkanAircraftLayout"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/lindsey_dense"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/leninistLayout">

        <Button
            android:background="@color/lepidolite_beresford"
            android:hint="@string/sherlock_amarillo_agrarian_vista"
            android:id="@+id/venalAcheView"
            android:layout_height="wrap_content"
            android:layout_width="454dp"
            android:text="@string/mortise_protein_dutiful_shunt"
            android:textColor="@color/polytope_infrastructure"
            app:layout_constraintBottom_toTopOf="@id/airmanNectarView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:background="@color/tipperary_deadlock"
            android:hint="@string/dumpy_pierre_alligator_wheller"
            android:id="@+id/airmanNectarView"
