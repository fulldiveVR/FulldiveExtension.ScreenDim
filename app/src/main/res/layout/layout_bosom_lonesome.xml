                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/girdlePhenolView" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:background="@color/earthmen_coffeecup_christopher"
            android:hint="@string/octahedron_galvanism_chart_disjunct"
            android:id="@+id/heedView"
            android:layout_height="wrap_content"
            android:layout_width="@dimen/amethyst_persia_padding"
            android:padding="@dimen/xavier_kiddie_size"
            android:text="@string/flotilla_bertram_armadillo"
            android:textColor="@color/georgetown_shareown"
            app:layout_constraintBottom_toTopOf="@id/hydroniumRebelledView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/soundLayout" />

        <CheckBox
            android:hint="@string/lilt_waylaid_raillery"
            android:id="@+id/hydroniumRebelledView"
            android:layout_height="wrap_content"
            android:layout_width="@dimen/cognate_sagittarius_width"
