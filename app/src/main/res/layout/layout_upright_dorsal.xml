<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <Button
        android:background="@color/terminable_partisan_jugging"
        android:hint="@string/boar_poole_interdict_token"
        android:id="@+id/recuseView"
        android:layout_height="wrap_content"
        android:layout_margin="40dp"
        android:layout_marginBottom="@dimen/kilovolt_adaptive_margin"
        android:layout_width="match_parent"
        android:text="@string/culinary_pharmacology_alienate"
        android:textColor="@color/pulmonary_turnkey_antiquary"
        android:textSize="@dimen/codfish_controlled_textsize"
        app:layout_constraintBottom_toTopOf="@id/newbornLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/newbornLayout"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        app:layout_constraintBottom_toTopOf="@id/enigmaLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/recuseView">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/murielLayout"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            app:layout_constraintBottom_toTopOf="@id/henbaneView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:background="@color/tabu_fair"
                android:hint="@string/lethe_stampede_exasperater"
                android:id="@+id/granteeView"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/studio_joint_padding"
                android:layout_width="@dimen/ramshackle_inviolable_height"
                android:text="@string/absentia_circulate_ionic"
                app:layout_constraintBottom_toTopOf="@id/unilateralEludeView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:background="@color/venezuela_saturnalia"
                android:hint="@string/earthman_cookie_burl_adjective"
                android:id="@+id/unilateralEludeView"
                android:layout_height="wrap_content"
                android:layout_width="@dimen/vega_memory"
                android:text="@string/wyeth_supervene_pablo"
                android:textColor="@color/exact_severe"
                android:textSize="@dimen/vancouver_confectionery_textsize"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/granteeView" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:background="@color/ponder_irresistible_knuckle"
            android:hint="@string/steve_collegiate_piggyback_truce"
            android:id="@+id/henbaneView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/therefor_parallax_quell_truancy"
            android:textColor="@color/syllabi_tendency"
            android:textSize="@dimen/parley_tine_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/murielLayout" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:gravity="top"
        android:id="@+id/enigmaLayout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:padding="@dimen/iceberg_roberts_padding"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/newbornLayout">

        <CheckedTextView
            android:background="@color/morocco_combatted_coronet"
            android:hint="@string/kinky_portmanteau_sonora_ymca"
            android:id="@+id/piccadillyView"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/relay_bloodshed"
            android:layout_width="wrap_content"
            android:text="@string/aventine_powder_reversal_transmissible"
            android:textColor="@color/shifty_dictatorial_afro" />

        <LinearLayout
            android:gravity="bottom|center_vertical"
            android:id="@+id/racketyTorridLayout"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="horizontal">

            <Button
                android:background="@color/conspiracy_champaign_monkeyflower"
                android:hint="@string/tread_calkins_divorce"
                android:id="@+id/reverberateLonghornView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:padding="@dimen/ambulatory_profound_padding"
                android:text="@string/telepathy_marks_turtleneck_courtesan"
                android:textColor="#86A8A2" />
        </LinearLayout>

        <LinearLayout
            android:gravity="bottom|center_vertical"
            android:id="@+id/sackWalpoleLayout"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="horizontal">

            <CheckedTextView
                android:background="@color/counterpart_eavesdropping"
                android:hint="@string/teledyne_accuracy_louisville"
                android:id="@+id/dialecticShoulderView"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/snore_coiffure"
                android:layout_width="wrap_content"
                android:text="@string/textbook_lipschitz_shopworn_floorboard"
                android:textColor="@color/univalent_ntis_nail" />
        </LinearLayout>

        <CheckBox
            android:background="@color/commonwealth_brigadier"
            android:hint="@string/natalie_remittance_analeptic"
            android:id="@+id/samuelsonView"
            android:layout_height="match_parent"
            android:layout_width="wrap_content"
            android:text="@string/deplane_blackfoot_cried_complaisant"
            android:textColor="#C2E0ED63"
            android:textSize="14sp" />

        <EditText
            android:background="@color/sabbath_alchemy"
            android:hint="@string/paraboloid_bologna_animist"
            android:id="@+id/repentLightenView"
            android:inputType="textCapCharacters"
            android:layout_height="match_parent"
            android:layout_width="wrap_content"
            android:text="@string/mona_optoisolate_winnipesaukee_tickle"
            android:textColor="@color/lotion_left_turbinate" />

        <CheckBox
            android:background="@color/brandt_tramp"
            android:hint="@string/once_afresh_totalitarian"
            android:id="@+id/totalSachsView"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/windup_wonderful_padding"
            android:layout_width="wrap_content"
            android:text="@string/ntis_circulate_quest_adam" />

        <EditText
            android:background="#CF1AA804"
            android:hint="@string/prorogue_padre_bronchitis"
            android:id="@+id/jiggleView"
            android:inputType="textMultiLine"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/obliterate_broody_squander"
            android:textColor="@color/icosahedron_moluccas" />

        <CheckBox
            android:background="@color/fussy_equivalent"
            android:hint="@string/naples_heave_lakeside"
            android:id="@+id/agnewView"
            android:layout_height="match_parent"
            android:layout_width="wrap_content"
            android:paddingBottom="18dp"
            android:text="@string/function_stephanie_coldhearted"
            android:textColor="@color/foxhole_whale_expedite"
            android:textSize="@dimen/glutamine_cultivar_textsize" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
