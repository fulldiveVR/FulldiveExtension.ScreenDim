        android:hint="@string/ritz_paycheck_buckwheat"
        android:id="@+id/recuperateView"
        android:layout_height="wrap_content"
        android:layout_width="840dp"
        android:text="@string/deuterium_congest_phillips"
        android:textColor="@color/incandescent_reagan"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@id/staveCavemanLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:gravity="end"
        android:id="@+id/staveCavemanLayout"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/murielLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/recuperateView">

        <TextView
            android:background="@color/endorse_cost"
            android:hint="@string/housekeep_monsanto_dispel"
            android:id="@+id/saturnaliaKitchenView"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@string/pablo_consult_rebecca_inconsequential"
